#include<stdio.h>
int main()
{
  int arr[100],i,max,min,n;
  printf("the no of elements in the array:");
  scanf("%d",&n);
  for(i=0;i<n;i++){
  printf("the element- %d:",i);
  scanf("%d",&arr[i]);
  }
  max=arr[0];
  min=arr[0];
  for(i=0;i<n;i++)
  {
      if (arr[i]>max)
        {
            max=arr[i];
        }
      if (arr[i]<min)
      {
        min=arr[i];
      }
  }
 printf("the max element is %d\n",max);
 printf("the min element is %d\n",min);
}

